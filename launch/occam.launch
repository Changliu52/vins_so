<launch>

<node pkg="feature_frontend" type="stereocam_node" name="feature" >
    <param name="camera_num_stereo" type="int" value="1"/>
    <rosparam subst_value="True">
        camera0:
            image_topic_left:       /occam_node/raw_image0
            image_topic_right:      /occam_node/raw_image1
            cam_config_file_left:   /home/gao/ws/src/vins/config/oscam/vins/cam_config_0.yaml
            cam_config_file_right:  /home/gao/ws/src/vins/config/oscam/vins/cam_config_1.yaml
            error_config_left:      /home/gao/ws/src/vins/config/oscam/vins/cam_0
            error_config_right:     /home/gao/ws/src/vins/config/oscam/vins/cam_0
            feature_config_file:    /home/gao/ws/src/vins/config/oscam/vins/feature_config.yaml
     </rosparam>
</node>


    <param name="num_of_cam" type="int" value="2" />
    <param name="camera_num_stereo" type="int" value="1"/>
    <rosparam subst_value="True">
        vins_config_file:           /home/gao/ws/src/vins/config/oscam/vins/vins_config.yaml

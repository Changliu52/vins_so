<node ns = "vins" name="vins" pkg="vins_so" type="vins_so" >
    <param name="vins_folder" type="string" value="/home/gao/ws/src/vins/" />
    <param name="num_of_cam" type="int" value="2" />
    <param name="camera_num_stereo" type="int" value="1"/>
    <rosparam subst_value="True">
        vins_config_file:           /home/gao/ws/src/vins/config/vi/vins/vins_config.yaml
        # individual setting for camera 0
        camera0:
            cam_config_file:        /home/gao/ws/src/vins/config/vi/vins/cam_config_left.yaml
        camera1:
            cam_config_file:        /home/gao/ws/src/vins/config/vi/vins/cam_config_right.yaml
    </rosparam>
</node>




